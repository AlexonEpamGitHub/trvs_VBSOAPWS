{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "DetailedErrors": true,
  "Authentication": {
    "Windows": {
      "Enabled": false
    }
  },
  "SoapService": {
    "MaxReceivedMessageSize": 2097152,
    "MaxArrayLength": 2097152,
    "MaxStringContentLength": 2097152
  }
}