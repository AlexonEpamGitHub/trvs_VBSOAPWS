{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient": "Warning",
      "Microsoft.AspNetCore.Authentication": "Information",
      "SoapCore": "Information",
      "Microsoft.AspNetCore.Routing": "Warning",
      "Microsoft.AspNetCore.Mvc": "Warning",
      "Microsoft.AspNetCore.Authorization": "Information"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss ",
        "UseUtcTimestamp": false,
        "ColorBehavior": "Default"
      }
    },
    "EventLog": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "Urls": "http://localhost:57114;https://localhost:57115",
  "Authentication": {
    "DefaultScheme": "Windows",
    "Schemes": {
      "Windows": {
        "DisplayName": "Windows Authentication",
        "PersistClaims": true,
        "PersistNtlm": true,
        "EnableKerberosCredentialDelegation": false
      },
      "JwtBearer": {
        "DisplayName": "JWT Bearer",
        "Authority": "",
        "Audience": "",
        "RequireHttpsMetadata": true,
        "SaveToken": true,
        "TokenValidationParameters": {
          "ValidateIssuer": true,
          "ValidateAudience": true,
          "ValidateLifetime": true,
          "ValidateIssuerSigningKey": true,
          "ClockSkew": "00:05:00"
        }
      }
    }
  },
  "Authorization": {
    "DefaultPolicy": "RequireAuthenticatedUser",
    "Policies": {
      "AllowAnonymous": {
        "Requirements": ["AllowAnonymous"]
      },
      "RequireWindowsAuth": {
        "Requirements": ["DenyAnonymous"],
        "AuthenticationSchemes": ["Windows"]
      },
      "SoapServiceAccess": {
        "Requirements": ["AllowAnonymous"]
      },
      "RequireJwtBearer": {
        "Requirements": ["DenyAnonymous"],
        "AuthenticationSchemes": ["JwtBearer"]
      }
    }
  },
  "Cors": {
    "DefaultPolicy": {
      "AllowedOrigins": ["*"],
      "AllowedMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "AllowedHeaders": ["*"],
      "AllowCredentials": false,
      "MaxAge": 86400,
      "PreflightMaxAge": 86400
    },
    "SoapPolicy": {
      "AllowedOrigins": ["*"],
      "AllowedMethods": ["POST", "GET", "OPTIONS"],
      "AllowedHeaders": ["Content-Type", "SOAPAction", "Authorization", "Accept", "Origin", "X-Requested-With"],
      "AllowCredentials": true,
      "MaxAge": 3600,
      "PreflightMaxAge": 3600,
      "ExposedHeaders": ["Content-Length", "Content-Type", "SOAPAction"]
    },
    "RestrictivePolicy": {
      "AllowedOrigins": [],
      "AllowedMethods": ["GET", "POST"],
      "AllowedHeaders": ["Content-Type", "Authorization"],
      "AllowCredentials": false,
      "MaxAge": 0
    }
  },
  "SoapEndpoints": {
    "BasePath": "/soap",
    "WsdlPath": "/wsdl",
    "EnableWsdl": true,
    "MaxRequestSize": 10485760,
    "RequestTimeout": "00:05:00",
    "AsyncOperationTimeout": "00:10:00",
    "EnableDetailedFaults": false,
    "SupportedBindings": ["BasicHttpBinding", "WSHttpBinding"],
    "MessageVersion": "Soap11",
    "EnableHttps": true,
    "RequireAuthentication": false,
    "BufferRequestContent": true,
    "MaxArrayLength": 16384,
    "MaxStringContentLength": 8192,
    "CaseSensitive": false,
    "ReaderQuotas": {
      "MaxDepth": 32,
      "MaxStringContentLength": 8192,
      "MaxArrayLength": 16384,
      "MaxBytesPerRead": 4096,
      "MaxNameTableCharCount": 16384
    },
    "Compression": {
      "Enabled": true,
      "Level": "Optimal",
      "MimeTypes": ["text/xml", "application/soap+xml"]
    }
  },
  "SoapServices": {
    "DefaultNamespace": "http://tempuri.org/",
    "EnableMetadata": true,
    "IncludeExceptionDetailInFaults": false,
    "CaseSensitive": false,
    "ModelBinderAttribute": true,
    "UseXmlSerializer": true,
    "XmlSerializerOptions": {
      "WriteXmlDeclaration": true,
      "OmitXmlDeclaration": false,
      "Indent": false,
      "Encoding": "utf-8"
    },
    "Bindings": {
      "BasicHttpBinding": {
        "Security": {
          "Mode": "None",
          "Transport": {
            "ClientCredentialType": "None"
          }
        },
        "MessageEncoding": "Text",
        "TransferMode": "Buffered",
        "TextEncoding": "utf-8"
      },
      "WSHttpBinding": {
        "Security": {
          "Mode": "Message",
          "Message": {
            "ClientCredentialType": "Windows"
          }
        },
        "MessageEncoding": "Text",
        "TransferMode": "Buffered",
        "TextEncoding": "utf-8"
      }
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:57114"
      },
      "Https": {
        "Url": "https://localhost:57115"
      }
    },
    "Limits": {
      "MaxRequestBodySize": 10485760,
      "RequestHeadersTimeout": "00:00:30",
      "KeepAliveTimeout": "00:02:00",
      "MaxConcurrentConnections": 100,
      "MaxConcurrentUpgradedConnections": 100,
      "MaxRequestHeaderCount": 100,
      "MaxRequestHeadersTotalSize": 32768,
      "MaxRequestLineSize": 8192,
      "MaxRequestBufferSize": 1048576,
      "MaxResponseBufferSize": 65536,
      "Http2": {
        "MaxStreamsPerConnection": 100,
        "HeaderTableSize": 4096,
        "MaxFrameSize": 16384,
        "MaxRequestHeaderFieldSize": 8192
      },
      "Http3": {
        "MaxRequestHeaderFieldSectionSize": 16384
      }
    },
    "AddServerHeader": false,
    "AllowSynchronousIO": false,
    "DisableStringReuse": false
  },
  "ForwardedHeaders": {
    "Enabled": true,
    "ForwardedProtoHeaderName": "X-Forwarded-Proto",
    "ForwardedForHeaderName": "X-Forwarded-For",
    "ForwardedHostHeaderName": "X-Forwarded-Host",
    "ForwardLimit": 1,
    "RequireHeaderSymmetry": false,
    "KnownProxies": [],
    "KnownNetworks": []
  },
  "RequestLocalization": {
    "DefaultRequestCulture": "en-US",
    "SupportedCultures": ["en-US", "en-GB"],
    "SupportedUICultures": ["en-US", "en-GB"],
    "ApplyCurrentCultureToResponseHeaders": true
  },
  "Globalization": {
    "Invariant": false,
    "PredefinedCulturesOnly": false
  },
  "HealthChecks": {
    "Enabled": true,
    "Path": "/health",
    "DetailedPath": "/health/detailed",
    "ReadyPath": "/health/ready",
    "LivePath": "/health/live",
    "Port": null,
    "Tags": [],
    "Timeout": "00:00:30",
    "DetailedErrors": false,
    "FailureStatus": 503,
    "CacheDuration": "00:00:10",
    "AllowCachingResponses": true
  },
  "ApiVersioning": {
    "DefaultApiVersion": "1.0",
    "AssumeDefaultVersionWhenUnspecified": true,
    "ApiVersionReader": "QueryString",
    "ApiVersionParameter": "version",
    "ReportApiVersions": true,
    "SupportedVersions": ["1.0", "2.0"],
    "DeprecatedVersions": []
  },
  "OpenApi": {
    "Enabled": true,
    "Title": "SOAP Service API",
    "Version": "v1",
    "Description": "ASP.NET Core SOAP Service",
    "IncludeXmlComments": true,
    "Contact": {
      "Name": "",
      "Email": "",
      "Url": ""
    },
    "License": {
      "Name": "",
      "Url": ""
    },
    "Servers": [
      {
        "Url": "https://localhost:57115",
        "Description": "Development HTTPS"
      },
      {
        "Url": "http://localhost:57114",
        "Description": "Development HTTP"
      }
    ]
  },
  "Swagger": {
    "Title": "SOAP Service API",
    "Version": "v1",
    "Description": "ASP.NET Core SOAP Service Documentation",
    "RoutePrefix": "swagger",
    "DocumentTitle": "SOAP Service API Documentation",
    "EnableAnnotations": true,
    "DisplayRequestDuration": true,
    "DocExpansion": "None",
    "DefaultModelsExpandDepth": 1,
    "DefaultModelRendering": "Example",
    "ShowExtensions": true,
    "ShowCommonExtensions": true,
    "EnableValidator": true,
    "SupportedSubmitMethods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
  },
  "ApplicationInsights": {
    "ConnectionString": "",
    "InstrumentationKey": "",
    "EnableAdaptiveSampling": true,
    "EnablePerformanceCounterCollectionModule": true,
    "EnableQuickPulseMetricStream": true,
    "EnableHeartbeat": true,
    "AddAutoCollectedMetricExtractor": true,
    "DeveloperMode": false,
    "EnableDependencyTrackingTelemetryModule": true,
    "EnableEventCounterCollectionModule": true
  },
  "DataProtection": {
    "ApplicationName": "SoapService",
    "KeyLifetime": "90.00:00:00",
    "KeyRingPath": null,
    "ProtectKeysWithDpapi": false,
    "ProtectKeysWithDpapiNG": false
  },
  "RateLimiting": {
    "EnableGlobalLimiter": false,
    "GlobalPolicy": "DefaultPolicy",
    "Policies": {
      "DefaultPolicy": {
        "PermitLimit": 100,
        "Window": "00:01:00",
        "ReplenishmentPeriod": "00:00:10",
        "TokensPerPeriod": 10,
        "AutoReplenishment": true
      },
      "SoapPolicy": {
        "PermitLimit": 50,
        "Window": "00:01:00",
        "ReplenishmentPeriod": "00:00:15",
        "TokensPerPeriod": 5,
        "AutoReplenishment": true
      }
    }
  },
  "OutputCaching": {
    "EnableGlobalCache": false,
    "DefaultPolicy": "DefaultCache",
    "Policies": {
      "DefaultCache": {
        "Duration": "00:05:00",
        "VaryByHeaders": ["Accept", "Content-Type"],
        "VaryByQueryKeys": ["*"]
      },
      "WsdlCache": {
        "Duration": "01:00:00",
        "VaryByHeaders": ["Accept"],
        "NoCache": false
      }
    }
  }
}