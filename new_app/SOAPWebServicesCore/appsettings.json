{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Windows": {
      "Enabled": true
    }
  },
  "Session": {
    "IdleTimeout": "00:20:00",
    "Cookie": {
      "HttpOnly": true,
      "IsEssential": true
    }
  },
  "Globalization": {
    "RequestEncoding": "utf-8",
    "ResponseEncoding": "utf-8"
  },
  "CustomErrors": {
    "Mode": "RemoteOnly"
  },
  "SoapService": {
    "WsiProfiles": "BasicProfile1_1",
    "EnvelopeVersion": "Soap12",
    "MaxReceivedMessageSize": 65536
  },
  "Cors": {
    "AllowedOrigins": ["*"],
    "AllowedMethods": ["POST", "OPTIONS"],
    "AllowedHeaders": ["Content-Type", "SOAPAction"]
  },
  "Security": {
    "RequireHttps": true,
    "MessageSecurity": {
      "Enabled": false
    }
  }
}