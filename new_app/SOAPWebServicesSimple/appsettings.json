{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "SessionTimeout": 20,
    "Authentication": {
      "Mode": "Windows"
    },
    "Globalization": {
      "RequestEncoding": "utf-8",
      "ResponseEncoding": "utf-8"
    }
  }
}