{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Windows": {
      "Enabled": true
    }
  },
  "Session": {
    "IdleTimeout": "00:20:00",
    "Cookie": {
      "HttpOnly": true,
      "IsEssential": true
    }
  }
}
