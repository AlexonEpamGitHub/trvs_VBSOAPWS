{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DetailedErrors": false,
  "Authentication": {
    "Windows": {
      "Enabled": true
    }
  },
  "Session": {
    "IdleTimeout": 20,
    "Mode": "InProc"
  }
}
