{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "DefaultScheme": "Windows"
  },
  "Session": {
    "Timeout": 20,
    "Mode": "InProc",
    "StateConnectionString": "tcpip=127.0.0.1:42424",
    "SqlConnectionString": "data source=127.0.0.1;user id=sa;password=",
    "Cookieless": false
  },
  "CustomErrors": {
    "Mode": "RemoteOnly"
  },
  "Authorization": {
    "AllowUsers": "*"
  },
  "Globalization": {
    "RequestEncoding": "utf-8",
    "ResponseEncoding": "utf-8"
  }
}